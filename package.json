{
  "name": "wimme.net",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run wimme.net:serve-ssr",
    "serve:ssr": "node dist/wimme.net/server/main.js",
    "build:ssr": "ng build && ng run wimme.net:server",
    "prerender": "ng run wimme.net:prerender",
    "prerender:wimme.net": "node createRoutesFile.mjs https://cms.wimme.net/system/json/ && ng run wimme.net:prerender --no-guess-routes --routes-file routes.txt",
    "prerender:skipdewinterdip.be": "node createRoutesFile.mjs https://cms.skipdewinterdip.be/system/json/ && ng run wimme.net:prerender --no-guess-routes --routes-file routes.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.6",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/elements": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@angular/platform-server": "^16.1.6",
    "@angular/router": "^16.1.6",
    "@nguniversal/express-engine": "^16.1.1",
    "express": "^4.15.2",
    "rxjs": "~7.8.0",
    "showdown": "^2.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular/cli": "~16.1.5",
    "@angular/compiler-cli": "^16.1.6",
    "@nguniversal/builders": "^16.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "@types/showdown": "^2.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}